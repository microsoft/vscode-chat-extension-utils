{
	"git.branchProtection": [
		"main"
	],
	"git.branchProtectionPrompt": "alwaysCommitToNewBranch",
	"files.trimTrailingWhitespace": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"search.exclude": {
		"dist": true
	},
	"files.readonlyInclude": {
		"src/util/**": true
	}
}